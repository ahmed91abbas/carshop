<!doctype html>
</html>
<head>
	<meta charset="UTF-8">
	<title> Car models </title>
</head>
<body>
	<% for(var i = 0; i < data.carshop.carmodels.length; i++) { %>
		<br><%= JSON.stringify(data.carshop.carmodels[i], null, 2) %>
	<% } %>

	<form action="/carmodels" method="post">
		<br><label>Brand </label><br>
		<input type="text" name="brand">
		<br><label>Model</label><br>
		<input type="text" name="model">
		<br><label>Price</label><br>
		<input type="number" name="price">
		<br><br><input type="submit" value="Add new car model">
	</form>
</body>
</html>